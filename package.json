{
  "name": "@doner/fetch",
  "version": "0.1.0",
  "description": "A handy Fetch API helper",
  "main": "index.js",
  "repository": "git@github.com:maximlysenko/doner-fetch.git",
  "scripts": {
    "clean": "rimraf dist",
    "size-limit": "./node_modules/.bin/size-limit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "build": "./node_modules/.bin/rollup -c --bundleConfigAsCjs",
    "prepare": "npm run clean && npm test && npm run build && npm run size-limit"
  },
  "typings": "index.d.ts",
  "keywords": [
    "fetch",
    "javascript",
    "typescript",
    "http",
    "request",
    "ajax"
  ],
  "files": [
    "dist",
    "index.d.ts"
  ],
  "author": "addition.mail.address@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "7.22.9",
    "@rollup/plugin-terser": "0.4.3",
    "@size-limit/preset-small-lib": "8.2.6",
    "babel-jest": "29.6.1",
    "esbuild": "0.18.12",
    "jest": "29.6.1",
    "jest-environment-jsdom": "29.6.1",
    "jest-fetch-mock": "3.0.3",
    "rimraf": "5.0.1",
    "rollup": "3.26.2"
  },
  "size-limit": [
    {
      "path": "./dist/index.js",
      "limit": "1 kB"
    }
  ]
}
